#include <stdio.h>

/*统计输入的字符数：版本1*/
main()
{
	long nc;
	
	nc=0;	/*初始化nc=0*/
	while(getchar() != EOF)	/*当获取到的值不等于EOF时，nc=nc+1*/
		nc++; 
	printf("%ld\n", nc);	/*按照格式打印输出nc,%ld =按照long十进制打印字符串*/
 } 
 
 #include <stdio.h>
/*统计输入的字符数：版本2*/
main()
{
	double nc;
	for(nc = 0; getchar() != EOF; ++nc)
		;
	printf("%.0f\n", nc);
}
